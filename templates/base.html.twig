<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {# 'fk_help_bundle' must match the first argument to addEntry() in webpack.config.js #}
            {{ encore_entry_link_tags('fk_help_bundle') }}

            <!-- Renders a link tag (if your module requires any CSS)
                 <link rel="stylesheet" href="/build/fk_help_bundle.css"> -->
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('fk_help_bundle') }}

            <!-- Renders fk_help_bundle.js & a webpack runtime.js file
                <script src="/build/runtime.js"></script>
                <script src="/build/fk_help_bundle.js"></script> -->
        {% endblock %}
    </body>
</html>
